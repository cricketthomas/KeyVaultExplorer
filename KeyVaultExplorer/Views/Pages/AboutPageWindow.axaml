    <Design.DataContext>
        <vm:AppViewModel />
    </Design.DataContext>

    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <Label Content="Key Vault Explorer" />
        <Label HorizontalAlignment="Center" Content="v0.1.0" />
    </StackPanel>

</Window>
